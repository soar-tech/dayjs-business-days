{
  "name": "dayjs-business-days",
  "version": "1.0.0-development",
  "description": "Day.js plugin to add support for calculating dates only accounting for Business days",
  "main": "index.min.js",
  "module": "index.min.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "babel src -d build",
    "minify": "terser build/index.js --compress --mangle --output index.min.js",
