    "days",
    "businessdays",
    "workingdays"
  ],
  "author": "Conor Watson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soar-tech/dayjs-business-days/issues"
  },
  "homepage": "https://github.com/soar-tech/dayjs-business-days#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "dayjs": "^1.8.24",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "terser": "^4.6.11"
  }
